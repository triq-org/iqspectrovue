{
  "name": "iqspectrovue",
  "version": "1.0.0",
  "private": true,
  "description": "I/Q Spectrogram viewer",
  "homepage": "https://github.com/triq-org/iqspectrovue#readme",
  "bugs": "https://github.com/triq-org/iqspectrovue/issues",
  "license": "GPL-2.0-or-later",
  "author": "Christian W. Zuckschwerdt <zany@triq.net>",
  "repository": "github:triq-org/iqspectrovue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "material-design-icons-iconfont": "^6.0.1",
    "spectroplot": "^1.1.0",
    "vue": "^2.7.0",
    "vuetify": "^2.2.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.6",
    "@vue/cli-plugin-eslint": "~5.0.6",
    "@vue/cli-service": "~5.0.6",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "sass": "~1.32.0",
    "sass-loader": "^13.0.0",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vuetify-loader": "^1.4.3",
    "worker-loader": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "warn"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
